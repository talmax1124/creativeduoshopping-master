    "@glidejs/glide": "^3.4.1",
    "@sentry/browser": "^6.4.0",
    "@sentry/tracing": "^6.4.0",
    "antd": "^4.15.3",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.25.0",
    "cloudinary-core": "^2.11.3",
    "colors": "^1.4.0",
    "darkmode-js": "^1.5.7",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-session": "^1.17.1",
    "google-translate": "^3.0.0",
